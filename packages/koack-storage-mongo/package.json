{
  "name": "koack-storage-mongo",
  "version": "0.0.0",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": "git@github.com:koack/koack.git",
  "homepage": "https://github.com/koack/koack",
  "engines": {
    "node": ">=8.3.0"
  },
  "main": "./index.js",
  "typings": "./dist/index.d.ts",
  "module:node": "./dist/index-node8.es.js",
  "module:node-dev": "./dist/index-node8-dev.es.js",
  "scripts": {
    "build": "pob-build && tsc -p tsconfig.build.json",
    "generate:api": "typedoc --out docs --tsconfig tsconfig.doc.json",
    "generate:docs": "rm -Rf docs ; yarn run generate:api",
    "lint": "npm run typescript-check && eslint --ext .ts  src/",
    "test": "echo \"No tests\"",
    "typescript-check": "tsc --noEmit",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "dependencies": {
    "koack-storage-utils": "0.0.0",
    "koack-types": "0.0.0",
    "mongodb": "^2.2.24"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-rc.3",
    "@types/mongodb": "^2.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0-beta.3",
    "babel-preset-latest-node": "2.0.0-beta.3",
    "eslint": "5.4.0",
    "eslint-config-pob": "22.0.3",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-typescript": "0.12.0",
    "pob-babel": "22.4.0",
    "prettier": "1.14.2",
    "typedoc": "0.12.0",
    "typescript": "3.0.1",
    "typescript-eslint-parser": "18.0.0"
  }

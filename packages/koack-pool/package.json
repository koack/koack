{
  "name": "koack-pool",
  "version": "0.0.0",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": "git@github.com:koack/koack.git",
  "homepage": "https://github.com/koack/koack",
  "engines": {
    "node": ">=8.3.0"
  },
  "main": "./index.js",
  "typings": "./dist/index.d.ts",
  "module:node": "./dist/index-node8.es.js",
  "module:node-dev": "./dist/index-node8-dev.es.js",
  "scripts": {
    "build": "pob-build && tsc -p tsconfig.build.json --declarationMap --emitDeclarationOnly",
    "generate:api": "typedoc --out docs --tsconfig tsconfig.build.json",
    "generate:docs": "rm -Rf docs ; yarn run generate:api",
    "lint": "tsc --noEmit && eslint --ext .ts  src/",
    "test": "echo \"No tests\"",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 100
  },
  "dependencies": {
    "koack-types": "^0.0.0",
    "nightingale-logger": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.5",
    "babel-preset-latest-node": "^2.0.0-beta.3",
    "eslint": "^4.19.1",
    "eslint-config-pob": "^20.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-prettier": "^2.6.1",
    "eslint-plugin-typescript": "^0.12.0",
    "pob-babel": "^22.2.5",
    "prettier": "^1.13.7",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.2",
    "typescript-eslint-parser": "^16.0.1"
  }
}
